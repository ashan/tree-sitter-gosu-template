{
    "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
    "grammars": [
        {
            "name": "gosu_template",
            "camelcase": "GosuTemplate",
            "title": "Gosu Template",
            "scope": "source.gosu.template",
            "file-types": [
                "gst"
            ],
            "injection-regex": "^gosu_template$",
            "class-name": "TreeSitterGosuTemplate"
        }
    ],
    "metadata": {
        "version": "0.1.0",
        "license": "MIT",
        "description": "Tree-sitter parser for Gosu Templates",
        "authors": [
            {
                "name": "Antigravity"
            }
        ],
        "links": {
            "repository": "https://github.com/tree-sitter/tree-sitter-gosu-template"
        }
    },
    "bindings": {
        "c": true,
        "go": true,
        "node": true,
        "python": true,
        "rust": true,
        "swift": true,
        "zig": false
    }
}