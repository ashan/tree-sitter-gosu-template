{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "gosu_template",
  "rules": {
    "template": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "content"
          },
          {
            "type": "SYMBOL",
            "name": "directive"
          },
          {
            "type": "SYMBOL",
            "name": "scriptlet"
          },
          {
            "type": "SYMBOL",
            "name": "expression"
          },
          {
            "type": "SYMBOL",
            "name": "interpolation"
          }
        ]
      }
    },
    "directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<%@"
        },
        {
          "type": "SYMBOL",
          "name": "_directive_body"
        },
        {
          "type": "STRING",
          "value": "%>"
        }
      ]
    },
    "_directive_body": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "params"
            },
            {
              "type": "STRING",
              "value": "("
            },
            {
              "type": "SYMBOL",
              "name": "parameter_list"
            },
            {
              "type": "STRING",
              "value": ")"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "extends"
            },
            {
              "type": "SYMBOL",
              "name": "type_reference"
            }
          ]
        }
      ]
    },
    "parameter_list": {
      "type": "PATTERN",
      "value": "[^\\)]+"
    },
    "type_reference": {
      "type": "PATTERN",
      "value": "[^%]+"
    },
    "scriptlet": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<%"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "scriptlet_content"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "%>"
        }
      ]
    },
    "scriptlet_content": {
      "type": "PATTERN",
      "value": "[^%]+"
    },
    "interpolation_content": {
      "type": "PATTERN",
      "value": "[^}]+"
    },
    "expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<%="
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "scriptlet_content"
          },
          "named": true,
          "value": "expression_content"
        },
        {
          "type": "STRING",
          "value": "%>"
        }
      ]
    },
    "interpolation": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "${"
        },
        {
          "type": "SYMBOL",
          "name": "interpolation_content"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "content"
    }
  ],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}